{
	"name": "svelte-file-tree-monorepo",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"build": "pnpm -r build",
		"build:packages": "pnpm -F \"./packages/**\" -r build",
		"build:preview": "pnpm build:packages && pnpm -F preview build",
		"check": "pnpm --parallel check",
		"format": "pnpm --parallel format",
		"lint": "pnpm --parallel format:check && eslint ."
	},
	"devDependencies": {
		"@changesets/cli": "^2.28.1",
		"@eslint/js": "^9.21.0",
		"eslint": "^9.21.0",
		"eslint-plugin-svelte": "^3.0.2",
		"globals": "^16.0.0",
		"playwright": "^1.50.1",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"better-sqlite3"
		]
	},
	"packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af"
}