{
	"name": "svelte-file-tree",
	"version": "0.12.0",
	"type": "module",
	"scripts": {
		"dev": "svelte-kit sync && svelte-package --watch",
		"build": "svelte-kit sync && svelte-package && publint",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "pnpm run build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"test": "vitest --run",
		"test:watch": "vitest --watch"
	},
	"files": [
		"dist",
		"!dist/**/*.spec.*",
		"!dist/**/*.test.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"types": "./dist/index.d.ts",
	"svelte": "./dist/index.js",
	"peerDependencies": {
		"svelte": "^5.31.0"
	},
	"dependencies": {
		"@atlaskit/pragmatic-drag-and-drop": "^1.7.4",
		"@tanstack/virtual-core": "^3.13.12",
		"esm-env": "^1.2.2"
	},
	"devDependencies": {
		"@sveltejs/kit": "^2.27.1",
		"@sveltejs/package": "^2.4.0",
		"@sveltejs/vite-plugin-svelte": "6.1.0",
		"@types/node": "^24.2.0",
		"@vitest/browser": "^3.2.4",
		"jsdom": "^26.1.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.12",
		"svelte": "5.38.0",
		"svelte-check": "^4.3.1",
		"vite": "^7.0.6",
		"vitest": "3.2.4",
		"vitest-browser-svelte": "^1.1.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/abdel-17/svelte-file-tree.git",
		"directory": "packages/svelte-file-tree"
	}
}
