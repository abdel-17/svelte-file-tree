{
	"name": "svelte-file-tree",
	"version": "0.3.0",
	"type": "module",
	"scripts": {
		"dev": "svelte-kit sync && svelte-package --watch",
		"build": "svelte-kit sync && svelte-package && publint",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "pnpm run build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"test": "vitest --run",
		"test:watch": "vitest --watch"
	},
	"files": [
		"dist",
		"!dist/**/*.spec.*",
		"!dist/**/*.test.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"types": "./dist/index.d.ts",
	"svelte": "./dist/index.js",
	"peerDependencies": {
		"svelte": "^5.20.0"
	},
	"dependencies": {
		"@atlaskit/pragmatic-drag-and-drop": "^1.6.1",
		"esm-env": "^1.2.2"
	},
	"devDependencies": {
		"@sveltejs/kit": "^2.20.8",
		"@sveltejs/package": "^2.3.11",
		"@sveltejs/vite-plugin-svelte": "5.0.3",
		"@types/node": "^22.15.17",
		"@vitest/browser": "^3.1.3",
		"jsdom": "^26.1.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.12",
		"svelte": "5.28.2",
		"svelte-check": "^4.1.7",
		"vite": "^6.3.5",
		"vitest": "3.1.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/abdel-17/svelte-file-tree.git",
		"directory": "packages/svelte-file-tree"
	}
}
